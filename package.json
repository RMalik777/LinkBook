		"@tailwindcss/vite": "4.1.16",
		"@typescript-eslint/parser": "^8.46.2",
		"clsx": "^2.1.1",
		"eslint": "^9.39.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"globals": "^16.5.0",
		"mode-watcher": "^1.1.0",
		"postcss": "^8.5.6",
