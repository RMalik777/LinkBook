		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "4.1.18",
		"@typescript-eslint/parser": "^8.53.0",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
